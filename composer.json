{
    "name": "joonas1234/nova-nested-form",
    "description": "A Laravel Nova package that allows you to create/update/delete nested related fields from a parent form.",
    "keywords": [
        "laravel",
        "nova",
        "nested",
        "relationship",
        "form"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "repositories": [{
        "type": "composer",
        "url": "https://nova.laravel.com"
    }],
    "autoload": {
        "psr-4": {
            "Joonas1234\\NestedForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joonas1234\\NestedForm\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova": "~2.0"
    }
}
